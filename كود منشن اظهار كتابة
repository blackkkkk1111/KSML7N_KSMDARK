'اختج على عيري المجنونة', 
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          'تم نيك امك بكل جنونة',
];
    }

    let randomIndex;
    do {
      randomIndex = Math.floor(Math.random() * messages.length);
    } while (messages[randomIndex] === previousMessage || !messages[randomIndex]);

    const message = messages[randomIndex];

    if (message && message.trim()) {
      previousMessage = message;
      messages.splice(randomIndex, 1);

      let sentSuccessfully = false;
      let attempts = 0;
      while (!sentSuccessfully && attempts < 5) {
        try {
          await sendMessage(message, CHANNEL_IDS[currentGroupIndex], mentionedUserIds);
          sentSuccessfully = true;

          currentGroupIndex = (currentGroupIndex + 1) % CHANNEL_IDS.length;
        } catch (err) {
          console.error('خطأ غير متوقع أثناء إرسال الرسالة:', err.message || err);
          attempts++;
          if (attempts < 5) {
            console.log(`إعادة المحاولة (${attempts})...`);
            await new Promise(resolve => setTimeout(resolve, 5000));
          } else {
            console.error('فشلت جميع المحاولات لإرسال الرسالة.');
          }
        }
      }

      const delay = calculateDelay(message);
      totalSeconds += delay / 1000;
      await new Promise(resolve => setTimeout(resolve, delay));
    } else {
      console.error('تم اختيار رسالة غير صالحة');
    }
  }
};

sendMessagesWithDelay().catch(err => {
  console.error('خطأ في إرسال الرسائل:', err.message || err);
});

const express = require('express');
const http = require('http');
const app = express();
app.get("/", (req, res) => {
  res.send(`
  <body>
    <center><h1>كسختو لحن</h1></center>
  </body>`);
});

setInterval(() => {
  const now = new Date();
  console.log(now.toLocaleTimeString());
}, 1000);

const server = http.createServer((req, res) => {
  res.setHeader('Content-Type', 'text/html');
  res.end(`
    <html>
      <head>
        <title>كسختو دارك</title>
      </head>
      <body style="margin: 0; padding: 0;">
        <iframe width="100%" height="100%" src="https://axocoder.vercel.app/" frameborder="0" allowfullscreen></iframe>
      </body>
    </html>`);
});

server.listen(8080, () => {
  console.log('I!!m ready nik ksm l7n..24H!!');
  app.listen(3000, () => console.log("I!!m Ready To nik ksm dark..! 24H"));
});

process.on('uncaughtException', (err) => {
  console.error('استثناء غير متوقع:', err.message || err);
});

process.on('unhandledRejection', (reason, promise) => {
  console.error('رفض غير معالج:', reason);
});

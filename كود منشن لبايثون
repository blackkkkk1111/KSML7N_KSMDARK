available_messages = messages.copy()  

headers = {
    'Authorization': TOKEN,
}

message_delay = 5  # تأخير بين كل رسالة

async def send_message_async(session, channel_id, content):
    url = f'https://discord.com/api/v10/channels/{channel_id}/messages'
    data = {'content': content}
    async with session.post(url, json=data) as response:
        return await response.json(), response.status

async def start_typing_async(session, channel_id):
    url = f'https://discord.com/api/v10/channels/{channel_id}/typing'
    async with session.post(url) as response:
        return response.status

async def main():
    async with aiohttp.ClientSession(headers=headers) as session:
        while True:
            for channel_id in CHANNEL_IDS:
                if available_messages:  
                    random_message = random.choice(available_messages)
                    mentions = " ".join([f'<@{user_id}>' for user_id in TARGET_USER_IDS])
                    content = f'{random_message} {mentions}'
                    
                  
                    await start_typing_async(session, channel_id)
                    await asyncio.sleep(2)
                    
                    
                    response_data, status = await send_message_async(session, channel_id, content)
                    if status == 200:
                        print(f'Message sent in channel {channel_id} to {", ".join(TARGET_USER_IDS)}: {content}')
                        await asyncio.sleep(message_delay)  
                        
                        
                        available_messages.remove(random_message)
                    elif status == 429:
                        retry_after = response_data.get('retry_after', 5)
                        print(f'Rate limit hit. Retrying after {retry_after} seconds.')
                        await asyncio.sleep(retry_after)  
                    else:
                        print(f'Failed to send message in channel {channel_id} to {", ".join(TARGET_USER_IDS)}: {status} - {response_data}')
                        await asyncio.sleep(message_delay)


if __name__ == '__main__':
    logging.basicConfig(level=logging.INFO)
    asyncio.run(main())
